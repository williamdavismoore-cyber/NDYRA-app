{
  "ci": {
    "collect": {
      "startServerCommand": "npx serve -s site -l 4173",
      "url": [
        "http://localhost:4173/",
        "http://localhost:4173/pricing.html",
        "http://localhost:4173/workouts/",
        "http://localhost:4173/workouts/category.html?c=hiit",
        "http://localhost:4173/app/timer/?src=demo:online_quick"
      ],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.85
          }
        ],
        "categories:accessibility": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "categories:best-practices": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "categories:seo": [
          "warn",
          {
            "minScore": 0.85
          }
        ]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./artifacts/lighthouse"
    }
  }
}